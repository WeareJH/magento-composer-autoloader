{
    "name": "jhhello/magento-composer-autoloader",
    "type":"magento-module",
    "description": "Magento Autoloader patch to require Composer's autoloader",
    "authors": [
        {
            "name": "Aydin Hassan",
            "email": "aydin@wearejh.com"
        }
    ],
    "require-dev": {
        "phpdocumentor/phpdocumentor": "v2.2.0"
    },
    "require": {
        "magento-hackathon/magento-composer-installer": "*"
    },
    "extra": {
        "map": [
            ["src/code/Varien", "app/code/local/Varien"]
        ]
    }
}
